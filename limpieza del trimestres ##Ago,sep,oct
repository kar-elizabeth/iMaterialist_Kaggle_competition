      "source": [
        "# Instalar el pd\n",
        "!pip install unidecode openpyxl\n",
        "\n",
        "import pandas as pd\n",
        "from unidecode import unidecode\n",
        "\n",
        "# 1) up\n",
        "from google.colab import files\n",
        "uploaded = files.upload()\n",
        "\n",
        "# 2) up name\n",
        "filename = list(uploaded.keys())[0]\n",
        "df = pd.read_excel(filename)\n",
        "\n",
        "# 3) clean text\n",
        "def limpiar_texto(x):\n",
        "    if pd.isnull(x):\n",
        "        return x\n",
        "    x = str(x)\n",
        "    x = x.strip()  # quitar espacios inicio-fin\n",
        "    x = unidecode(x)  # quitar acentos\n",
        "    x = \" \".join(x.split())  # eliminar dobles espacios o tabuladores\n",
        "    x = x.upper()  # mayúsculas\n",
        "    return x\n",
        "\n",
        "# 4) clean a todo el DataFrame\n",
        "df = df.applymap(limpiar_texto)\n",
        "\n",
        "# 5) Separar columna \"Ruta GFA\" en dos columnas\n",
        "if \"Ruta GFA\" in df.columns:\n",
        "    df[\"RUTA_ORIGEN\"] = df[\"Ruta GFA\"].apply(lambda x: x.split(\"-\")[0].strip() if pd.notnull(x) and \"-\" in x else x)\n",
        "    df[\"RUTA_DESTINO\"] = df[\"Ruta GFA\"].apply(lambda x: x.split(\"-\")[1].strip() if pd.notnull(x) and \"-\" in x else x)\n",
        "else:\n",
        "    print(\"La columna 'Ruta GFA' no existe en el archivo. Verifica el nombre exacto.\")\n",
        "\n",
        "# 6) save data\n",
        "output_name = \"BASE_LIMPIA.xlsx\"\n",
        "df.to_excel(output_name, index=False)\n",
        "\n",
        "# 7) download\n",
        "files.download(output_name)\n",
        "\n",
        "print(\"Proceso de limpieza finalizado. Archivo listo para descargar.\")\n",
        "\n",
        "\n",
        "\n",
        "\n"
      ]
    },
